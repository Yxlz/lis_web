package com.cdxt.lisweb.model.right;

import java.util.ArrayList;
import java.util.List;

import com.cdxt.lisweb.model.tree.Node;

/**
 * 菜单树节点
 * 
 * @author lixying
 * @date 2017年5月3日 下午4:37:12
 * @since 1.0.0
 */
public class RightNode extends Node{


  private String rightName;

  private String pinyin;

  private String remark;

  private String parentCode;

  private String leafNum;

  private String url;

  private String icon;

  private String sortNo;

  /**
   * 权限的ID
   */
  private String rId;

  private List<RightNode> children = new ArrayList<RightNode>();

  public RightNode(String id, String text) {
    this.id = id;
    this.text = text;
  }


  public String getrId() {
    return rId;
  }


  public void setrId(String rId) {
    this.rId = rId;
  }



  

  public List<RightNode> getChildren() {
    return children;
  }

  public void addChild(RightNode n) {
    this.children.add(n);
  }

  public void setChildren(List<RightNode> children) {
    this.children = children;
  }


  public String getRightName() {
    return rightName;
  }

  public void setRightName(String rightName) {
    this.rightName = rightName;
  }

  public String getPinyin() {
    return pinyin;
  }

  public void setPinyin(String pinyin) {
    this.pinyin = pinyin;
  }

  public String getRemark() {
    return remark;
  }

  public void setRemark(String remark) {
    this.remark = remark;
  }

  public String getParentCode() {
    return parentCode;
  }

  public void setParentCode(String parentCode) {
    this.parentCode = parentCode;
  }

  public String getLeafNum() {
    return leafNum;
  }

  public void setLeafNum(String leafNum) {
    this.leafNum = leafNum;
  }

  public String getUrl() {
    return url;
  }

  public void setUrl(String url) {
    this.url = url;
  }

  public String getIcon() {
    return icon;
  }

  public void setIcon(String icon) {
    this.icon = icon;
  }

  public String getSortNo() {
    return sortNo;
  }

  public void setSortNo(String sortNo) {
    this.sortNo = sortNo;
  }


}
