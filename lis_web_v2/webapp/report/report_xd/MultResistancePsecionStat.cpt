<![CDATA[select T1.科室,T1.总数 ,T2.细菌,
--case when T2.检测菌株 is null then 0 else T2.检测菌株 end as CRABA,
--case when T3.检测菌株 is null then 0 else T3.检测菌株 end as MRSA,
--case when T4.检测菌株 is null then 0 else T4.检测菌株 end as CRPAE,
--case when T5.检测菌株 is null then 0 else T5.检测菌株 end as CRECEO,
--case when T6.检测菌株 is null then 0 else T6.检测菌株 end as CRKPN,
--case when T7.检测菌株 is null then 0 else T7.检测菌株 end as VREFA,
--case when T8.检测菌株 is null then 0 else T8.检测菌株 end as VREFM
 T2.检测菌株  as CRABA,
 T3.检测菌株  as MRSA,
 T4.检测菌株  as CRPAE,
 T5.检测菌株  as CRECEO,
 T6.检测菌株  as CRKPN,
 T7.检测菌株  as VREFA,
 T8.检测菌株  as VREFM
from  (select COUNT(科室) as 总数,科室
   from (
  SELECT p.p_section as 科室,
 case when instr(m.item_name,'%')>0 
   then substr(m.item_name,instr(m.item_name,'%')+1,instr(m.item_name,'(',instr(m.item_name,'%'),1)-instr(m.item_name,'%')-1)   
 when instr(m.item_name,'(') = 1  
   then substr(m.item_name,instr(m.item_name,')')+1,instr(m.item_name,'(',instr(m.item_name,')'),1)-instr(m.item_name,')')-1)   
else m.item_name end AS 细菌
FROM LIS_INSPEC_RESULT_HIST_MICRO m 
  INNER JOIN LIS_INSPEC_GENERAL_INFO_HIST g ON g.INSPEC_NO=m.INSPEC_NO
    AND g.DEV_CODE=m.DEV_CODE
    AND m.TIME_REPORT=to_date(g.input_date,'yyyy-MM-dd')
    AND to_char(g.time_report,'yyyy-mm-dd') between '${startDate}'  and '${endDate}'
    and m.type='阳性'
    and m.mdr_type is not null
  inner JOIN LIS_INSPEC_PATIENT p ON p.id=g.PATIENT_ID 
  GROUP BY item_name,g.request_no,p.p_section
)M1  group by  M1.科室
)T1 left join
(select 细菌,COUNT(细菌) as 检测菌株,科室
   from (
  SELECT p.p_section as 科室,
 case when instr(m.item_name,'%')>0 
   then substr(m.item_name,instr(m.item_name,'%')+1,instr(m.item_name,'(',instr(m.item_name,'%'),1)-instr(m.item_name,'%')-1)   
 when instr(m.item_name,'(') = 1  
   then substr(m.item_name,instr(m.item_name,')')+1,instr(m.item_name,'(',instr(m.item_name,')'),1)-instr(m.item_name,')')-1)   
else m.item_name end AS 细菌
FROM LIS_INSPEC_RESULT_HIST_MICRO m 
  INNER JOIN LIS_INSPEC_GENERAL_INFO_HIST g ON g.INSPEC_NO=m.INSPEC_NO
    AND g.DEV_CODE=m.DEV_CODE
    AND m.TIME_REPORT=to_date(g.input_date,'yyyy-MM-dd')
   AND to_char(g.time_report,'yyyy-mm-dd') between '${startDate}'  and '${endDate}'
    and m.type='阳性'
   and m.mdr_type = 'CRABA'
  inner JOIN LIS_INSPEC_PATIENT p ON p.id=g.PATIENT_ID 
  GROUP BY item_name,g.request_no,m.mdr_type,p.p_section
)M1  where M1.细菌 = '鲍曼不动杆菌'  group by  M1.细菌,M1.科室
)T2  on T1.科室 = T2.科室
left join
(select 细菌,COUNT(细菌) as 检测菌株,科室
   from (
  SELECT p.p_section as 科室,
 case when instr(m.item_name,'%')>0 
   then substr(m.item_name,instr(m.item_name,'%')+1,instr(m.item_name,'(',instr(m.item_name,'%'),1)-instr(m.item_name,'%')-1)   
 when instr(m.item_name,'(') = 1  
   then substr(m.item_name,instr(m.item_name,')')+1,instr(m.item_name,'(',instr(m.item_name,')'),1)-instr(m.item_name,')')-1)   
else m.item_name end AS 细菌
FROM LIS_INSPEC_RESULT_HIST_MICRO m 
  INNER JOIN LIS_INSPEC_GENERAL_INFO_HIST g ON g.INSPEC_NO=m.INSPEC_NO
    AND g.DEV_CODE=m.DEV_CODE
    AND m.TIME_REPORT=to_date(g.input_date,'yyyy-MM-dd')
   AND to_char(g.time_report,'yyyy-mm-dd') between '${startDate}'  and '${endDate}'
    and m.type='阳性'
    and m.mdr_type = 'MRSA'
  inner JOIN LIS_INSPEC_PATIENT p ON p.id=g.PATIENT_ID 
  GROUP BY item_name,g.request_no,m.mdr_type,p.p_section
)M1  where M1.细菌 = '金黄色葡萄球菌'  group by  M1.细菌,M1.科室
)T3  on T1.科室 = T3.科室
left join
(select 细菌,COUNT(细菌) as 检测菌株,科室
   from (
  SELECT p.p_section as 科室,
 case when instr(m.item_name,'%')>0 
   then substr(m.item_name,instr(m.item_name,'%')+1,instr(m.item_name,'(',instr(m.item_name,'%'),1)-instr(m.item_name,'%')-1)   
 when instr(m.item_name,'(') = 1  
   then substr(m.item_name,instr(m.item_name,')')+1,instr(m.item_name,'(',instr(m.item_name,')'),1)-instr(m.item_name,')')-1)   
else m.item_name end AS 细菌
FROM LIS_INSPEC_RESULT_HIST_MICRO m 
  INNER JOIN LIS_INSPEC_GENERAL_INFO_HIST g ON g.INSPEC_NO=m.INSPEC_NO
    AND g.DEV_CODE=m.DEV_CODE
    AND m.TIME_REPORT=to_date(g.input_date,'yyyy-MM-dd')
   AND to_char(g.time_report,'yyyy-mm-dd') between '${startDate}'  and '${endDate}'
    and m.type='阳性'
     and m.mdr_type = 'CRPAE'
  inner JOIN LIS_INSPEC_PATIENT p ON p.id=g.PATIENT_ID 
  GROUP BY item_name,g.request_no,m.mdr_type,p.p_section
)M1  where M1.细菌 = '铜绿假单胞菌'  group by  M1.细菌,M1.科室
)T4  on T1.科室 = T4.科室
left join
(select 细菌,COUNT(细菌) as 检测菌株,科室
   from (
  SELECT p.p_section as 科室,
 case when instr(m.item_name,'%')>0 
   then substr(m.item_name,instr(m.item_name,'%')+1,instr(m.item_name,'(',instr(m.item_name,'%'),1)-instr(m.item_name,'%')-1)   
 when instr(m.item_name,'(') = 1  
   then substr(m.item_name,instr(m.item_name,')')+1,instr(m.item_name,'(',instr(m.item_name,')'),1)-instr(m.item_name,')')-1)   
else m.item_name end AS 细菌
FROM LIS_INSPEC_RESULT_HIST_MICRO m 
  INNER JOIN LIS_INSPEC_GENERAL_INFO_HIST g ON g.INSPEC_NO=m.INSPEC_NO
    AND g.DEV_CODE=m.DEV_CODE
    AND m.TIME_REPORT=to_date(g.input_date,'yyyy-MM-dd')
   AND to_char(g.time_report,'yyyy-mm-dd') between '${startDate}'  and '${endDate}'
    and m.type='阳性'
     and m.mdr_type = 'CRECO'
  inner JOIN LIS_INSPEC_PATIENT p ON p.id=g.PATIENT_ID 
  GROUP BY item_name,g.request_no,m.mdr_type,p.p_section
)M1  where M1.细菌 = '大肠埃希菌'  group by  M1.细菌,M1.科室
)T5  on T1.科室 = T5.科室
left join
(select 细菌,COUNT(细菌) as 检测菌株,科室
   from (
  SELECT p.p_section as 科室,
 case when instr(m.item_name,'%')>0 
   then substr(m.item_name,instr(m.item_name,'%')+1,instr(m.item_name,'(',instr(m.item_name,'%'),1)-instr(m.item_name,'%')-1)   
